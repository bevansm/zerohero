{
  "name": "zerohero",
  "version": "0.0.0",
  "description": "dumb fun discord bot",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "env-cmd node dist/index.js",
    "start:prod": "node dist/index.js",
    "dev": "env-cmd ts-node --files src/index.ts",
    "dev:watch": "nodemon",
    "validate": "tslint --project tsconfig.json",
    "prod": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mebird/zerohero.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.21",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@types/cron": "^1.7.2",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.4",
    "@types/qs": "^6.9.4",
    "@types/ws": "^7.2.6",
    "env-cmd": "^10.1.0",
    "nodemon": "^2.0.3",
    "ts-node": "^8.9.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "nodemonConfig": {
    "watch": [
      "src/index.ts"
    ],
    "exec": "npm run dev",
    "ext": "ts",
    "ignore": [
      "*.spec.ts"
    ],
    "delay": "3"
  }
}
